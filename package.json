{
  "name": "idai",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "yarn api:generate",
    "start": "ng serve",
    "prebuild": "yarn api:generate",
    "build": "ng build --prod",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "api:generate": "openapi-generator-cli generate --skip-validate-spec -g typescript-angular -i https://api.test.idai.world/public/openapi.json -o src/app/generated/search --additional-properties apiModulePrefix=Search,supportsES6=true,ngVersion=8.1.2",
    "postapi:generate": "echo 'export const ERRORUNKNOWN = {};' > src/app/generated/search/model/eRRORUNKNOWN.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@ngxd/core": "^7.0.3",
    "angulartics2": "^7.5.2",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "custom-event-polyfill": "^1.0.7",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "popper.js": "^1.14.7",
    "rxjs": "~6.5.2",
    "simplebar": "^3.1.5",
    "slick-carousel": "^1.8.1",
    "tslib": "^1.9.0",
    "whatwg-fetch": "^3.0.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@openapitools/openapi-generator-cli": "^2.4.13",
    "@types/faker": "^5.1.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.123",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "faker": "^5.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
